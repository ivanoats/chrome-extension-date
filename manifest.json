{
  "name": "Injecta",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Injecting Date",
  "homepage_url": "http://ivanstorck.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject fake Date"
  },
  "content_scripts": [
		{
		 "matches": ["https://*/*","http://*/*"],
		 "js": ["inject.js"]
		}
	],
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}